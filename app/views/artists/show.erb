<h1><%=@artist.name%></h1>
<br/>
<h3>Songs</h3>
<% @artist.songs.each do |song| %>
    <a href="/songs/<%=song.slug%>"><li><%= song.name %> </li></a>
    <% end %>
<br/>
<h3>Genre</h3>
<% @artist.genres.each do |genre| %>
    <a href="/genres/<%=genre.slug%>"><li><%= genre.name %> </li></a>
    <% end %>